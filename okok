       Text = 'Auto Sell Solaris',
       Default = false,
       Tooltip = '',
})
local function toggleChanged()
    if Toggles.Solaris.Value then
         spawn(function()
          local args = {
    [1] = false
}
          while Toggles.Solaris.Value do
              game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("ExchangeMoney"):FireServer(unpack(args))
              wait(1)
