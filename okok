RightGroupBox4:AddToggle('HAHA', {
       Text = 'Kaitan Bitcoin Miner ⛏️(BETA)',
       Default = false,
       Tooltip = '',
})
local function toggleChanged()
    if Toggles.HAHA.Value then
         spawn(function()
          local args = {
    [1] = false
}
          while Toggles.HAHA.Value do
            game.Players.LocalPlayer:Kick("you have been banned for:cheating suspicion")
              wait(1)
            end
        end)
    end
end
Toggles.HAHA:OnChanged(toggleChanged)



