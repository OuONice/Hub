-------------------------------------  Auto  -------------------------------------------------------------------------
local RightGroupBox4 = Tabs.Teleport:AddRightGroupbox('       \\\\  Auto Anything  //')
RightGroupBox4:AddToggle('Claim', {
    Text = 'Auto Claim Star',
    Default = false,
    Tooltip = '',
})
local function toggleChanged()
    if Toggles.Claim.Value then
        spawn(function()
            while Toggles.Claim.Value do
            game:GetService("ReplicatedStorage").Events.ClaimFreeBoostStar:FireServer()
                wait(1) -- เพิ่ม wait เพื่อป้องกันการเกิดอาการแลค
            end
        end)
    end
end
Toggles.Claim:OnChanged(toggleChanged)
---------------------------------------------  Auto  Claim Bitcoin  -------------------------------------------

RightGroupBox4:AddToggle('BitCoin', {
       Text = 'Auto Sell BittCoin',
       Default = false,
       Tooltip = '',
})
local function toggleChanged()
    if Toggles.BitCoin.Value then
         spawn(function()
           local args = {
    [1] = true
}
          while Toggles.BitCoin.Value do
          game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("ExchangeMoney"):FireServer(unpack(args))
              wait(1)
            end
        end)
    end
end
Toggles.BitCoin:OnChanged(toggleChanged)

-------------------------------------------  Auto  Solaris  -------------------------------------------------------------------------
