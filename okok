local LeftGroupBox = Tabs.Teleport:AddLeftGroupbox('         \\\\   World   //')


local Button1 = LeftGroupBox:AddButton('Sea 1', function()
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelMain")
end)

local Button2 = LeftGroupBox:AddButton('Sea 2', function()
     game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa")

